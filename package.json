{
  "name": "backend-fundamentals-bedu",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "DEBUG=backend-fundamentals-bedu:* nodemon ./bin/www",
    "model:generate": "sequelize model:generate",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo",
    "seed:create": "sequelize seed:create --name",
    "db:setup": "npm run migrate && npm run seed",
    "db:drop": "sequelize db:drop",
    "db:reset": "npm run db:drop && npm run db:setup"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.2.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "6.3.0",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
